{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node -r ts-node/register -r tsconfig-paths/register ./src/App.ts",
    "start:watch": "nodemon",
    "build": "tsc -p ."
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  },
  "dependencies": {
    "apollo-server-express": "^2.5.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^1.30.1",
    "express": "^4.16.4",
    "express-graphql": "^0.8.0",
    "graphql": "^14.3.0",
    "helmet": "^3.18.0",
    "morgan": "^1.9.1",
    "pg": "^7.10.0",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.8.5",
    "socket.io": "^2.2.0",
    "sqlite3": "^4.0.7",
    "type-graphql": "^0.17.3",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.17",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/compression": "^0.0.36",
    "@types/graphql": "^14.2.0",
    "@types/helmet": "^0.0.43",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.0",
    "@types/sequelize": "^4.27.49",
    "@types/socket.io": "^2.1.2",
    "@types/validator": "^10.11.0",
    "nodemon": "^1.19.0",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0"
  }
}
