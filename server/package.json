{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node -r ts-node/register ./src/App.ts",
    "start:watch": "nodemon",
    "build": "tsc -p ."
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  },
  "dependencies": {
    "@types/node": "^12.0.0",
    "compression": "^1.7.4",
    "express": "^4.16.4",
    "express-graphql": "^0.8.0",
    "graphql": "^14.3.0",
    "helmet": "^3.18.0",
    "morgan": "^1.9.1",
    "pg": "^7.10.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.8.5",
    "socket.io": "^2.2.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/compression": "^0.0.36",
    "@types/helmet": "^0.0.43",
    "@types/morgan": "^1.7.35",
    "@types/sequelize": "^4.27.49",
    "@types/socket.io": "^2.1.2",
    "nodemon": "^1.19.0",
    "ts-node": "^8.1.0"
  }
}
